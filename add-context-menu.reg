"Icon"="C:\\ImageConverter\\icon.ico"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.WEBP\command]
@="\"C:\\ImageConverter\\ImageConverter.exe\" \"%1\" webp"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.HEIC]
@="Convert to HEIC"
"Icon"="C:\\ImageConverter\\icon.ico"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.HEIC\command]
@="\"C:\\ImageConverter\\ImageConverter.exe\" \"%1\" heic"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.ICO]
