Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\*\shell\ImageConverter]
"MUIVerb"="Image Converter"
"Icon"="C:\\ImageConverter\\icon.ico"
"SubCommands"="ImageConverter.PNG;ImageConverter.JPEG;ImageConverter.WEBP;ImageConverter.HEIC;ImageConverter.ICO"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.PNG]
@="Convert to PNG"
"Icon"="C:\\ImageConverter\\icon.ico"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.PNG\command]
@="\"C:\\ImageConverter\\ImageConverter.exe\" \"%1\" png"

[HKEY_LOCAL_MACHINE\Software\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ImageConverter.JPEG]
